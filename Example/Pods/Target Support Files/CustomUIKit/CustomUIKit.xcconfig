CONFIGURATION_BUILD_DIR = $PODS_SHARED_BUILD_DIR/CustomUIKit
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Private" "${PODS_ROOT}/Headers/Private/CustomUIKit" "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CustomUIKit" "${PODS_ROOT}/Headers/Public/HMSegmentedControl" "${PODS_ROOT}/Headers/Public/Masonry" "${PODS_ROOT}/Headers/Public/UIKit+CUAddtion"
LIBRARY_SEARCH_PATHS = $(inherited) "$PODS_SHARED_BUILD_DIR/HMSegmentedControl"
OTHER_LDFLAGS = -framework "CoreGraphics" -framework "Foundation" -framework "UIKit"
PODS_ROOT = ${SRCROOT}
PODS_SHARED_BUILD_DIR = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PRODUCT_BUNDLE_IDENTIFIER = org.cocoapods.${PRODUCT_NAME:rfc1034identifier}
SKIP_INSTALL = YES
